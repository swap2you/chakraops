{
  "name": "chakraops-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 vite",
    "build": "tsc -b && cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 vite build",
    "lint": "eslint .",
    "preview": "cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "check": "npm run test && npm run build",
    "live:check": "vitest run src/test/liveSchema.test.ts",
    "ci:health": "npm run typecheck && npm run test && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.57.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.0.1",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0",
    "@types/node": "^20.10.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
